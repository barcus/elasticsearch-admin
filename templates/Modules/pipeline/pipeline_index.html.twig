{% extends 'base.html.twig' %}
{% import 'Import/app_import.html.twig' as appImport %}

{% block head_title %}{{ 'pipelines'|trans }}{% endblock %}

{% block heading_1 %}
    {{ appImport.heading({'level': 1, 'title': 'pipelines'|trans}) }}
{% endblock %}

{% block tabs %}
    {% include 'Modules/pipeline/pipeline_tabs.html.twig' with {'active': 'list'} %}
{% endblock %}

{% block main_content %}
    {% if 0 < pipelines.total %}
        {% embed 'Embed/card_embed.html.twig' %}
            {% import 'Import/app_import.html.twig' as appImport %}
            {% block content %}
                {{ appImport.heading({'level': 3, 'title': 'list'|trans, 'badge': pipelines.total}) }}

                {{ appImport.paginator(pipelines) }}

                {% include 'Modules/pipeline/pipeline_list.html.twig' with {'pipelines': pipelines} %}

                {{ appImport.paginator(pipelines) }}
            {% endblock %}
        {% endembed %}
    {% endif %}
{% endblock %}
